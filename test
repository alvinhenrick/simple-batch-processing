CREATE TABLE DB.TABLE_NAME_NEW AS   
SELECT  FIELD_NAME,  
SUM (CASE WHEN IND_PYMT_CAT_Y = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN ALLOW_INCL_MBR_COST_SHR_AMT ELSE 0 END) AS MED_ALLOWED_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_Y = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN CHRG_AMT ELSE 0 END) AS MED_CHARGE_AMT_360,
SUM (CASE WHEN IND_PYMT_CAT_Y = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN COB_PAID_AMT ELSE 0 END) AS MED_COB_PAID_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_Y = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN COINS_AMT ELSE 0 END) AS MED_MBR_COINS_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_Y = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN COPAY_AMT ELSE 0 END) AS MED_MBR_COPAY_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_Y = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN DEDUCT_AMT ELSE 0 END) AS MED_MBR_DEDUCT_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_Y = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN MBR_RESP_AMT ELSE 0 END) AS MED_MBR_RESP_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_Y = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN NET_PAID_AMT ELSE 0 END) AS MED_NET_PAID_AMT_360,
SUM(CASE WHEN IND_PYMT_CAT_Y = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN UCR_AMT ELSE 0 END) AS MED_UCR_AMT_360,
SUM (CASE WHEN IND_PYMT_CAT_X = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN ALLOW_INCL_MBR_COST_SHR_AMT ELSE 0 END) AS REJ_ALLOWED_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_X = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN CHRG_AMT ELSE 0 END) AS REJ_CHARGE_AMT_360,
SUM (CASE WHEN IND_PYMT_CAT_X = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN COB_PAID_AMT ELSE 0 END) AS REJ_COB_PAID_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_X = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN COINS_AMT ELSE 0 END) AS REJ_MBR_COINS_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_X = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN COPAY_AMT ELSE 0 END) AS REJ_MBR_COPAY_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_X = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN DEDUCT_AMT ELSE 0 END) AS REJ_MBR_DEDUCT_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_X = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN MBR_RESP_AMT ELSE 0 END) AS REJ_MBR_RESP_AMT_360, 
SUM (CASE WHEN IND_PYMT_CAT_X = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN NET_PAID_AMT ELSE 0 END) AS REJ_NET_PAID_AMT_360,
SUM (CASE WHEN IND_PYMT_CAT_X = 1 AND EARLY_SERV_EFF_DATE BETWEEN DATE360 AND LAG_DATE THEN UCR_AMT ELSE 0 END) AS REJ_UCR_AMT_360
FROM DB.TABLE_NAME
GROUP BY FIELD_NAME
